{
  "name": "vite-plugin-bundle-css",
  "version": "0.0.1",
  "description": "A Vite plugin to bundle CSS files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kuoruan/vite-plugin-bundle-css.git"
  },
  "homepage": "https://github.com/kuoruan/vite-plugin-bundle-css",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "test:update": "vitest --run --update",
    "lint": "eslint ."
  },
  "keywords": [
    "vite",
    "plugin",
    "bundle",
    "css"
  ],
  "files": [
    "dist"
  ],
  "author": "Xingwang Liao<kuoruan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.1.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.3.3",
    "rollup": "^4.20.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0"
  },
  "peerDependencies": {
    "rollup": ">=2.0.0",
    "vite": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "rollup": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  }
}
