{
  "name": "vite-plugin-bundle-css",
  "version": "0.0.1",
  "description": "A Vite plugin to bundle CSS files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kuoruan/vite-plugin-bundle-css.git"
  },
  "homepage": "https://github.com/kuoruan/vite-plugin-bundle-css",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --run"
  },
  "keywords": [
    "vite",
    "plugin",
    "bundle",
    "css"
  ],
  "files": ["dist"],
  "author": "Xingwang Liao<kuoruan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.1.0",
    "rollup": "^4.20.0",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0"
  },
  "peerDependencies": {
    "vite": ">=2.0.0",
    "rollup": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "rollup": {
      "optional": true
    }
  }
}
